(bind dist (func ({ ax ay } { bx by }) (do
    (bind { dx dy } { (- bx ax) (- by ay) })
	(sqrt (+ (* dx dx) (* dy dy)))
)))

(bind point-cmp-orig (func (origin px py) (do
    (bind dx (dist origin px))
    (bind dy (dist origin py))
    (< dx dy)
)))

(bind point-cmp-int (point-cmp-orig { 0 0 }))
(bind point-cmp-real (point-cmp-orig { 0.0 0.0 }))

(bind pts-int [ { 3 3 } { 1 1 } { 2 2 } ])
(bind pts-real [ { 1.0 0.0 } { 0.0 1.0 } { 1.0 1.0 } { 0.0 0.0 }])

(bind main (func ()
	(printf "%s, %s" { "Hello" "World" })
))
