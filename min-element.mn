(bind min-element`` (func (x y)
	(if (< x y)
		x
		y
	)
))

(bind min-element` (func (element array)
	(bind first (car array))
	(if (empty array)
		(min-element`` element first)
		(if (< element first)
			(min-element` element (cdr array))
			(min-element` first (cdr array))
		)
	)
))

(bind min-element (func (array)
	(min-element` (car array) (cdr array))
))